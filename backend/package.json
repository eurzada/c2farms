{
  "name": "c2farms-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:migrate": "npx prisma migrate dev --schema=src/prisma/schema.prisma",
    "db:push": "npx prisma db push --schema=src/prisma/schema.prisma",
    "db:seed": "node src/prisma/seed.js",
    "db:studio": "npx prisma studio --schema=src/prisma/schema.prisma",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pdfmake": "^0.2.12",
    "socket.io": "^4.8.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "prisma": "^5.22.0",
    "supertest": "^7.2.2",
    "vitest": "^4.0.18"
  }
}
